{
    "name": "pastetheplan",
    "displayName": "pastetheplan",
    "description": "Paste the Plan Connector",
    "version": "0.1.0",
    "publisher": "drewsk",
    "engines": {
        "vscode": "^1.28.0",
        "sqlops": "*"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pastePlan",
        "onCommand:extension.pastePlanCopy"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
        {
            "command": "extension.pastePlan",
            "title": "Paste the Plan: Open in Browser"
        },
        {
            "command": "extension.pastePlanCopy",
            "title": "Paste the Plan: Copy Link"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install && node ./node_modules/sqlops/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "sqlops": "github:anthonydresser/sqlops-extension-sqlops",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "request": "^2.88.0",
        "request-promise-native": "^1.0.5",
        "clipboardy": "^1.2.3"
    }
}
